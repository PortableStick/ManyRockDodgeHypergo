class Link {
    field Rock rock;
    field Laser laser;
    field Link next;

    constructor Link newRockLink(Rock _r) {
        let rock = _r;
        return this;
    }

    constructor Link newLaserLink(Laser _l) {
        let laser = _l;
        return this;
    }

    method void dispose() {
        do Memory.deAlloc(this);
        return;
    }

    method void setNext(Link _l) {
        let next = _l;
        return;
    }

    method Link getNext() {
        return next;
    }

    method Rock getRock() {
        return rock;
    }

    method Laser getLaser() {
        return laser;
    }
}